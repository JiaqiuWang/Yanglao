                  "调料", "白面", "白面"]
    var_temp = random.choice(var_ill_id)
    return var_temp


def random_from():
    var_ill_id = ["超市快递", "闪电购", "超市快递", "超市快递", "超市快递", "超市快递",
                  "超级快递", "超级快递"]
    var_temp = random.choice(var_ill_id)
