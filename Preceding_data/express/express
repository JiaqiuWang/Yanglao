def random_type_express():
    var_ill_id = ["大米", "食用油", "蔬菜", "水果", "鸡蛋", "食用油", "食用油", "鸡蛋",
                  "鸡蛋", "鸡蛋", "大米"]
    var_temp = random.choice(var_ill_id)
    return var_temp


def random_from():
    var_ill_id = ["快递家", "小区快递", "闪电购", "快递家", "超市快递", "超市快递", "快递家", "快递家"]
    var_temp = random.choice(var_ill_id)
    return var_temp

def random_pay_service():
    var_ill_id = ["Alipay", "PayPal", "PayPal", "PayPal", "Alipay", "银联支付"]
    var_temp = random.choice(var_ill_id)
    return var_temp

def random_quantity():
    var_ill_id = ["more", "less", "more", "more", "more", "more", "less", "more"]
    var_temp = random.choice(var_ill_id)
    return var_temp

def get_deliver_address(type_express):
    if type_express == "蔬菜" or type_express == "水果":
        deliver_add = "Pension Agency"
    else:
        deliver_add = "Home"
    return deliver_add


def get_charge_quantity(quantity):
    if quantity == "more":
        charge = "high"
    else:
        charge = "low"
    return charge

