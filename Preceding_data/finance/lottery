def random_location():
    var_ill_id = ["Home", "Community", "Community", "Community", "Community",
                  "Community", "Community", "Community",
                  "Pension Agency", "Home", "Home", "Home", "Corporation", "Mall"]
    var_temp = random.choice(var_ill_id)
    return var_temp


