p2:

def get_balance(type_lottery):
    if type_lottery == "查询中奖信息":
        balance_my = "unchanged"
    elif type_lottery == "购买彩票":
        balance_my = "expense"
    elif type_lottery == "出售彩票" or type_lottery == "领取中奖金额":
        balance_my = "income"
    else:
        balance_my = "unchanged"
    return balance_my


