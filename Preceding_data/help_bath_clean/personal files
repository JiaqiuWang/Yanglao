P1:
r_hour = int(random.randint(6, 12))


def random_from():
    var_ill_id = ["社区宝", "养老宝", "爱家养老", "web_yang_lao", "社区宝"]
    var_temp = random.choice(var_ill_id)
    return var_temp


def random_pay_service():
    # var_ill_id = ["Alipay", "Wechat", "Credit Card",  "PayPal"]
    var_ill_id = ["Alipay", "PayPal", "PayPal", "PayPal", "Alipay", "银联支付"]
    var_temp = random.choice(var_ill_id)
    return var_temp

def get_location(from_sa):
    # 获取集合
    location = "Home"
    if from_sa == "社区宝":
        location = "Community"
    if from_sa == "养老宝" or from_sa == "web_yang_lao":
        location = "Pension Agency"
    return location
